{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build Debug",
            "command": "dotnet",
            "type": "process",
            "args": [ "build" ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": "$tsc"
        },
        {
            "label": "build Release",
            "command": "dotnet",
            "type": "process",
            "args": [ "build", "-c", "Release" ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": "$tsc"
        }
    ]
}